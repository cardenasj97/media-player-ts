  }\n\n    toggleMute() {\n        if (this.media.muted) {\n            this.unmute();\n        } else {\n            this.mute();\n        }\n    }\n}\n\nexport default MediaPlayer;"},"lineCount":null}},"error":null,"hash":"bd78f1d4fe9c2ee8b004614398088d89","cacheData":{"env":{}}}